<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<OverflowToolbar class="customHeaderBar" id="headerProfileBar">
		<Image id="avatarlog" class="imageAvatar sapThemeBrand-asColor" src="{userSessions>photo}"/>
		<Button class="sapThemeBrand-asColor" id="backArrow" icon="sap-icon://slim-arrow-left" press="onPressBack" type="Transparent"
			tooltip="{i18n>ttBack}">
			<layoutData>
				<OverflowToolbarLayoutData priority="NeverOverflow"/>
			</layoutData>
		</Button>
		<Button class="sapThemeBrand-asColor" id="home" icon="sap-icon://home" type="Transparent" press="gotoSFSF">
			<layoutData>
				<OverflowToolbarLayoutData priority="Disappear"/>
			</layoutData>
		</Button>
		<!--<Image class="imageCorporative" src="images/sapCompanyLogo.png"/>-->
		<Image id="companyLogo" class="imageCorporative" src="/images/logo.png"/>
		<!--<Image src="{userSessions>logo}"/>-->
		<ToolbarSpacer/>
		<Label class="sapUiVisibleOnlyOnDesktop sapThemeText sapThemeBrand-asColor"
			text="{i18n>appTitle} -">
			<layoutData>
				<OverflowToolbarLayoutData priority="NeverOverflow"/>
			</layoutData>
		</Label>
		<Label class="sapThemeText sapThemeBrand-asColor" id="employeeInfo" text="">
